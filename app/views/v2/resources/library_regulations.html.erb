<% content_for :page_header do %>
  <br />
  <ul class="breadcrumb">
    <li><%= link_to 'Documentation', v2_root_path %> <span class="divider">/</span></li>
    <li><%= link_to 'Resources', v2_resources_path %> <span class="divider">/</span></li>
    <li class="active">Regulation</li>
  </ul>
  <div class="row">
    <div class="span12">
      <h1>GET regulations</h1>
    </div>
  </div>
<% end %>

<div class="row">
  <div class="span8">
    <h3>Resource URL</h3>
    <p>/api/v2/regulations.json</p>
    <br />
    <h3>Example Request</h3>
    <table class="table">
      <tr>
        <td>GET</td>
        <td>/api/v1/regulations.xml</td>
      </tr>
    </table>
    <p><pre class="prettyprint">
&lt;Response&gt;
  &lt;Regulations&gt;
    &lt;Regulation id=&quot;1&quot;&gt;
      &lt;Name&gt;FAR&lt;/Name&gt;
      &lt;Title&gt;Federal Acquisition Regulation&lt;/Title&gt;
      &lt;Url&gt;https://www.acquisition.gov/far&lt;/Url&gt;
    &lt;/Regulation&gt;
  &lt;/Regulations&gt;
&lt;/Response&gt;</pre></p>
    <h3>Example Request with Pagination</h3>
    <table class="table">
      <tr>
        <td>GET</td>
        <td>/api/v1/regulations.xml?page=1</td>
      </tr>
    </table>
    <p><pre class="prettyprint">
&lt;Response&gt;
  &lt;Pagination&gt;
    &lt;CurrentPage&gt;1&lt;/CurrentPage&gt;
    &lt;TotalPages&gt;5&lt;/TotalPages&gt;
    &lt;TotalResults&gt;100&lt;/TotalResults&gt;
  &lt;/Pagination&gt;
  &lt;Regulations&gt;
    &lt;Regulation id=&quot;1&quot;&gt;
      &lt;Name&gt;FAR&lt;/Name&gt;
      &lt;Title&gt;Federal Acquisition Regulation&lt;/Title&gt;
      &lt;Url&gt;https://www.acquisition.gov/far&lt;/Url&gt;
    &lt;/Regulation&gt;
  &lt;/Regulations&gt;
&lt;/Response&gt;</pre></p>
  </div>
  <div class="span4">
    <div class="well">
      <h3>Resource Information</h3>
      <table class="table">
        <tr>
          <td>Response Formats</td>
          <td>json</td>
        </tr>
        <tr>
          <td>HTTP Method</td>
          <td>GET</td>
        </tr>
        <tr>
          <td>Response Object</td>
          <td><%= link_to 'Regulations', v2_resource_objects_library_regulation_path %></td>
        </tr>
      </table>
    </div>
  </div>
</div>
