<% content_for :page_header do %>
  <br />
  <ul class="breadcrumb">
    <li><%= link_to 'Documentation', v2_root_path %> <span class="divider">/</span></li>
    <li><%= link_to 'Resources', v2_resources_path %> <span class="divider">/</span></li>
    <li class="active">Procurement Document</li>
  </ul>
  <div class="row">
    <div class="span12">
      <h1>PUT procurement_documents/:token</h1>
    </div>
  </div>
<% end %>

<div class="row">
  <div class="span8">
    <h3>Resource URL</h3>
    <p>/api/v1/procurement_documents/:token.format</p>
    <br />
    <h3>Parameters</h3>
    <table class="table">
      <tr>
        <td><strong>token</strong></td>
        <td><span class="label label-important">required</span></td>
        <td>The token of the desired Procurement Document.</td>
      </tr>
      <tr>
        <td><strong>Number</strong></td>
        <td><span class="label label-important">required</span></td>
        <td>Procurement Document Number</td>
      </tr>
    </table>
    <h3>Example Request</h3>
    <table class="table">
      <tr>
        <td>PUT</td>
        <td>/api/v1/procurement_documents/c9de297b5ba56cbe22537138e9302a5d.xml</td>
      </tr>
    </table>
    <pre class="prettyprint">
&lt;Request&gt;
  &lt;Number&gt;FY2012-ABC123-0001&lt;/Number&gt;
&lt;/Request&gt;</pre>
    <br />
    <pre class="prettyprint">
&lt;Response&gt;
  &lt;ProcurementDocument&gt;
    &lt;Token&gt;c9de297b5ba56cbe22537138e9302a5d&lt;/Token&gt;
    &lt;Number&gt;FY2012-ABC123-0001&lt;/Number&gt;
    &lt;RunRules&gt;false&lt;/RunRules&gt;
    &lt;RulesRanAt&gt;07/01/2012 08:00:03&lt;/RulesRanAt&gt;
    &lt;CreatedAt&gt;01/01/2012 08:00:03&lt;/CreatedAt&gt;
  &lt;/ProcurementDocument&gt;
&lt;/Response&gt;</pre>
  </div>
  <div class="span4">
    <div class="well">
      <h3>Resource Information</h3>
      <table class="table">
        <tr>
          <td>Request Type</td>
          <td><%= link_to 'Protected', v2_authentication_request_types_path %></td>
        </tr>
        <tr>
          <td>Response Formats</td>
          <td>xml</td>
        </tr>
        <tr>
          <td>HTTP Method</td>
          <td>PUT</td>
        </tr>
        <tr>
          <td>Response Object</td>
          <td><%= link_to 'Procurement Document', v2_resource_objects_procurement_document_procurement_document_path %></td>
        </tr>
      </table>
    </div>
  </div>
</div>
