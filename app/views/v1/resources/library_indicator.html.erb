<% content_for :page_header do %>
  <br />
  <ul class="breadcrumb">
    <li><%= link_to 'Documentation', v1_root_path %> <span class="divider">/</span></li>
    <li><%= link_to 'Resources', v1_resources_path %> <span class="divider">/</span></li>
    <li class="active">Indicator</li>
  </ul>
  <div class="row">
    <div class="span12">
      <h1>GET indicators/:id</h1>
    </div>
  </div>
<% end %>

<div class="row">
  <div class="span8">
    <h3>Resource URL</h3>
    <p>/api/v1/indicators/:id.format</p>
    <br />
    <h3>Parameters</h3>
    <table class="table">
      <tr>
        <td><strong>id</strong></td>
        <td><span class="label label-important">required</span></td>
        <td>The unique id of the desired indicator.</td>
      </tr>
    </table>
    <h3>Example Request</h3>
    <table class="table">
      <tr>
        <td>GET</td>
        <td>/api/v1/indicators/1.xml</td>
      </tr>
    </table>
    <br />
    <pre class="prettyprint">
&lt;Response&gt;
  &lt;Indicator id=&quot;1&quot;&gt;
    &lt;Name&gt;Organization&lt;/Name&gt;
    &lt;DataType&gt;String&lt;/DataType&gt;
    &lt;Constraint&gt;PICK_ONE&lt;/Constraint&gt;
    &lt;Values&gt;
      &lt;Value id=&quot;1&quot;&gt;Department of Defense&lt;/Value&gt;
      &lt;Value id=&quot;2&quot;&gt;NASA&lt;/Value&gt;
      &lt;Value id=&quot;3&quot;&gt;Coast Guard&lt;/Value&gt;
    &lt;/Values&gt;
  &lt;/Indicator&gt;
&lt;/Response&gt;</pre>
  </div>
  <div class="span4">
    <div class="well">
      <h3>Resource Information</h3>
      <table class="table">
        <tr>
          <td>Request Type</td>
          <td><%= link_to 'Non Authenticated', v1_authentication_request_types_path %></td>
        </tr>
        <tr>
          <td>Response Formats</td>
          <td>xml</td>
        </tr>
        <tr>
          <td>HTTP Method</td>
          <td>GET</td>
        </tr>
        <tr>
          <td>Response Object</td>
          <td><%= link_to 'Indicator', v1_resource_objects_library_indicator_path %></td>
        </tr>
      </table>
    </div>
  </div>
</div>
