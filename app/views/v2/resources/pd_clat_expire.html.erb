    <table class="table">
      <tr>
        <td><strong>token</strong></td>
        <td><span class="label label-important">required</span></td>
        <td>The procurement document token.</td>
      </tr>
      <tr>
        <td><strong>id</strong></td>
        <td><span class="label label-important">required</span></td>
        <td>The unique id of the desired CLAT.</td>
      </tr>
    </table>
    <h3>Example Request</h3>
    <table class="table">
      <tr>
        <td>PUT</td>
        <td>/api/v1/procurement_documents/c9de297b5ba56cbe22537138e9302a5d/clats/1/expire.xml</td>
      </tr>
    </table>
    <br />
    <pre class="prettyprint">
&lt;Response&gt;
  &lt;Clat id=&quot;1&quot;&gt;
    &lt;Token&gt;7fd5e89f7c85d7ab82b79047bc56c3dd&lt;/Token&gt;
