<% content_for :page_header do %>
  <br />
  <ul class="breadcrumb">
    <li><%= link_to 'Documentation', v2_root_path %> <span class="divider">/</span></li>
    <li><%= link_to 'Resources', v2_resources_path %> <span class="divider">/</span></li>
    <li class="active">Clause Revisions</li>
  </ul>
  <div class="row">
    <div class="span12">
      <h1>GET clauses/:id/revisions</h1>
    </div>
  </div>
<% end %>

<div class="row">
  <div class="span8">
    <h3>Resource URL</h3>
    <p>/api/v1/clauses/:id/revisions.format</p>
    <br />
    <h3>Parameters</h3>
    <table class="table">
      <tr>
        <td><strong>id</strong></td>
        <td><span class="label label-important">required</span></td>
        <td>The unique id of the desired clause.</td>
      </tr>
    </table>
    <h3>Example Request</h3>
    <table class="table">
      <tr>
        <td>GET</td>
        <td>/api/v1/clauses/1/revisions.xml</td>
      </tr>
    </table>
    <br />
    <pre class="prettyprint">
&lt;Response&gt;
  &lt;Revisions&gt;
    &lt;Revision id=&quot;1&quot; isEditable=&quot;false&quot;&gt;
      &lt;Title&gt;Clause Title&lt;/Title&gt;
      &lt;Body&gt;
        &lt;![CDATA[Clause Text]]&gt;
      &lt;/Body&gt;
      &lt;EffectiveStartDate&gt;01/01/2012&lt;/EffectiveStartDate&gt;
      &lt;EffectiveEndDate/&gt;
      &lt;Fillins&gt;
        &lt;Fillin id=&quot;1&quot; name=&quot;Employee Count&quot; source=&quot;Government&quot; dataType=&quot;Numeric&quot;&gt;&lt;/Fillin&gt;
        &lt;Fillin id=&quot;2&quot; name=&quot;Location&quot; source=&quot;Government&quot; dataType=&quot;Choose&quot;&gt;
          &lt;Options&gt;
            &lt;Option&gt;On-Site&lt;/Option&gt;
            &lt;Option&gt;Off-Site&lt;/Option&gt;
          &lt;/Options&gt;
        &lt;/Fillin&gt;
      &lt;/Fillins&gt;
    &lt;/Revision&gt;
  &lt;/Revisions&gt;
&lt;/Response&gt;</pre>
  </div>
  <div class="span4">
    <div class="well">
      <h3>Resource Information</h3>
      <table class="table">
        <tr>
          <td>Request Type</td>
          <td><%= link_to 'Non Authenticated', v2_authentication_request_types_path %></td>
        </tr>
        <tr>
          <td>Response Formats</td>
          <td>xml</td>
        </tr>
        <tr>
          <td>HTTP Method</td>
          <td>GET</td>
        </tr>
        <tr>
          <td>Response Object</td>
          <td><%= link_to 'Clause Revisions', v2_resource_objects_library_clause_revision_path %></td>
        </tr>
      </table>
    </div>
  </div>
</div>
