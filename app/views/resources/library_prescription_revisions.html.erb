<% content_for :page_header do %>
  <br />
  <ul class="breadcrumb">
    <li><%= link_to 'Documentation', root_path %> <span class="divider">/</span></li>
    <li><%= link_to 'Resources', resources_path %> <span class="divider">/</span></li>
    <li class="active">Prescription Revisions</li>
  </ul>
  <div class="row">
    <div class="span12">
      <h1>GET prescriptions/:id/revisions</h1>
    </div>
  </div>
<% end %>

<div class="row">
  <div class="span8">
    <h3>Resource URL</h3>
    <p><%= request.protocol %><%= request.host %>/api/v1/prescriptions/:id/revisions.format</p>
    <br />
    <h3>Parameters</h3>
    <table class="table">
      <tr>
        <td>
          <strong>id</strong><br />
          required
        </td>
        <td>The unique id of the desired prescription.</td>
      </tr>
    </table>
    <h3>Example Request</h3>
    <table class="table">
      <tr>
        <td>GET</td>
        <td><%= request.protocol %><%= request.host %>/api/v1/prescriptions/6/revisions.xml</td>
      </tr>
    </table>
    <br />
    <pre class="prettyprint"><%= @response %></pre>
  </div>
  <div class="span4">
    <div class="well">
      <h3>Resource Information</h3>
      <table class="table">
        <tr>
          <td>Request Type</td>
          <td><%= link_to 'Non Authenticated', authentication_request_types_path %></td>
        </tr>
        <tr>
          <td>Response Formats</td>
          <td>xml</td>
        </tr>
        <tr>
          <td>HTTP Method</td>
          <td>GET</td>
        </tr>
        <tr>
          <td>Response Object</td>
          <td><%= link_to 'Prescription Revisions', resource_objects_library_prescription_revision_path %></td>
        </tr>
      </table>
    </div>
  </div>
</div>
