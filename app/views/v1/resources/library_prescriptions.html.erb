<% content_for :page_header do %>
  <br />
  <ul class="breadcrumb">
    <li><%= link_to 'Documentation', v1_root_path %> <span class="divider">/</span></li>
    <li><%= link_to 'Resources', v1_resources_path %> <span class="divider">/</span></li>
    <li class="active">Prescription</li>
  </ul>
  <div class="row">
    <div class="span12">
      <h1>GET prescriptions</h1>
    </div>
  </div>
<% end %>

<div class="row">
  <div class="span8">
    <h3>Resource URL</h3>
    <p>/api/v1/prescriptions.format</p>
    <br />
    <h3>Example Request</h3>
    <table class="table">
      <tr>
        <td>GET</td>
        <td>/api/v1/prescriptions.xml</td>
      </tr>
    </table>
    <p><pre class="prettyprint">
&lt;Response&gt;
  &lt;Prescriptions&gt;
    &lt;Prescription id=&quot;1&quot;&gt;
      &lt;Number&gt;52.202-1&lt;/Number&gt;
      &lt;Regulation id=&quot;1&quot;&gt;FAR&lt;/Regulation&gt;
      &lt;Revision id=&quot;1&quot;&gt;
        &lt;Body&gt;
          &lt;![CDATA[Prescription Text]]&gt;
        &lt;/Body&gt;
        &lt;EffectiveStartDate&gt;01/01/2012&lt;/EffectiveStartDate&gt;
        &lt;EffectiveEndDate/&gt;
      &lt;/Revision&gt;
    &lt;/Prescription&gt;
  &lt;/Prescriptions&gt;
&lt;/Response&gt;</pre></p>
    <h3>Example Request with Pagination</h3>
    <table class="table">
      <tr>
        <td>GET</td>
        <td>/api/v1/prescriptions.xml?page=1</td>
      </tr>
    </table>
    <p><pre class="prettyprint">
&lt;Response&gt;
  &lt;Pagination&gt;
    &lt;CurrentPage&gt;1&lt;/CurrentPage&gt;
    &lt;TotalPages&gt;5&lt;/TotalPages&gt;
    &lt;TotalResults&gt;100&lt;/TotalResults&gt;
  &lt;/Pagination&gt;
  &lt;Prescriptions&gt;
    &lt;Prescription id=&quot;1&quot;&gt;
      &lt;Number&gt;52.202-1&lt;/Number&gt;
      &lt;Regulation id=&quot;1&quot;&gt;FAR&lt;/Regulation&gt;
      &lt;Revision id=&quot;1&quot;&gt;
        &lt;Body&gt;
          &lt;![CDATA[Prescription Text]]&gt;
        &lt;/Body&gt;
        &lt;EffectiveStartDate&gt;01/01/2012&lt;/EffectiveStartDate&gt;
        &lt;EffectiveEndDate/&gt;
      &lt;/Revision&gt;
    &lt;/Prescription&gt;
  &lt;/Prescriptions&gt;
&lt;/Response&gt;</pre></p>
  </div>
  <div class="span4">
    <div class="well">
      <h3>Resource Information</h3>
      <table class="table">
        <tr>
          <td>Request Type</td>
          <td><%= link_to 'Non Authenticated', v1_authentication_request_types_path %></td>
        </tr>
        <tr>
          <td>Response Formats</td>
          <td>xml</td>
        </tr>
        <tr>
          <td>HTTP Method</td>
          <td>GET</td>
        </tr>
        <tr>
          <td>Optional Parameters</td>
          <td>
            <em>page</em> - enables pagination. (example page=1)
          </td>
        </tr>
        <tr>
          <td>Response Object</td>
          <td><%= link_to 'Prescriptions', v1_resource_objects_library_prescription_path %></td>
        </tr>
      </table>
    </div>
  </div>
</div>
